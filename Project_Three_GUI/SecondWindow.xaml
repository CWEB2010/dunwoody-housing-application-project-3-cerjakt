                <TabItem x:Name="backgroundcolor" Header="Residents" Background="#FFAA1D2C" Foreground="White" BorderBrush="Black" Content="put your elements here" Margin="0"/>
                <TabItem x:Name="tabIitemContent" Header="New Resident" Foreground="White" BorderBrush="Black" Background="#FFAA1D2C" Margin="0">
                
                </TabItem>
            </TabControl>

        </StackPanel>

</Window>
